<div class="masonry">
  {{#each ownedItems.skill}}
  {{#if this}}
  <div class="masonry-item">
    <h2>{{localize (concat 'RQG.Actor.Skills.SkillCategory.' @key)}} ({{#with @root.characterData.skillCategoryModifiers}}{{lookup this @key}}{{/with}})</h2>
    <div class="grid skill">
      {{#each this}}
        {{#unless data.data.runes}}
        <div data-item-id="{{id}}" title="Description" class="skill contextmenu"
          {{#if data.data.journalId}} data-journal-pack="{{data.data.journalPack}}" data-journal-id="{{data.data.journalId}}"{{/if}}>
          <img class="item" src="{{img}}">
        </div>
        <div data-item-id="{{id}}" class="skill contextmenu" data-item-roll title="Roll" title="Roll">
          {{data.data.skillName}}{{#if data.data.specialization}} ({{data.data.specialization}}){{/if}}
          {{#each data.data.runes}}
            <img class="rune" src="{{runeImg this}}" title="{{this}}">
          {{/each}}
        </div>
      <div data-item-id="{{id}}" class="skill contextmenu {{#if data.data.hasExperience}}experienced{{/if}}" data-item-roll title="Roll">
        <span class="text-right nowrap">{{data.data.chance}}%</span>
      </div>
        {{/unless}}
      {{/each}}
    </div>
  </div>
  {{/if}}
  {{/each}}
</div>
