<h2>{{localize "RQG.ActiveEffect.DebugTab.Header"}}</h2>
<table>
  <thead>
  <tr>
    <th class="text-left">{{localize "RQG.ActiveEffect.DebugTab.Label"}}</th>
    <th class="text-left">{{localize "RQG.ActiveEffect.DebugTab.Disabled"}}</th>
    <th class="text-left">{{localize "RQG.ActiveEffect.DebugTab.StartTime"}}</th>
    <th class="text-left">{{localize "RQG.ActiveEffect.DebugTab.KeyValueMode"}}</th>
    <th class="text-left"></th>
  </tr>
  </thead>
  {{#each data.effects}}
  <tbody data-effect-id="{{_id}}">
  <tr data-actor-effect-edit>
    <td>{{label}}</td>
    <td>{{yes-no disabled}}</td>
    <td>{{duration.startTime}}</td>
    <td>
      {{#each changes}}
        {{key}} - {{value}} - {{mode}}<br>
      {{/each}}</td>
    <td class="text-right"><a title="Delete Effect" data-actor-effect-delete><i class="fas fa-trash"></i></a></td>
    </tr>

  </tbody>
  {{/each}}

</table>
