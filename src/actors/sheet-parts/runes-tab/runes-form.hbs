<div class="grid {{#if wizard}}opposed-rune-select{{else}}opposed-rune{{/if}} item-list">
  <h2 class="fullrow">{{localize "RQG.Actor.Rune.Form"}}</h2>

  {{#with embeddedItems.rune.form.Man}}
    {{>"systems/rqg/actors/sheet-parts/runes-tab/grid-rune.hbs"
      rune=this
      wizard=../wizard
      forChoice=../forChoice
      checkedProperty=../checkedProperty}}
  {{/with}}
  <i class="fas fa-arrows-alt-h"></i>
  {{#with embeddedItems.rune.form.Beast}}
    {{>"systems/rqg/actors/sheet-parts/runes-tab/grid-rune.hbs"
      rune=this
      wizard=../wizard
      forChoice=../forChoice
      checkedProperty=../checkedProperty}}
  {{/with}}

  {{#each embeddedItems.rune.form}}
    {{#unless (eq @key "Man")}}
      {{#unless (eq @key "Beast")}}
        {{#with this}}
          {{>"systems/rqg/actors/sheet-parts/runes-tab/grid-rune.hbs"
            rune=this
            wizard=../wizard
            forChoice=../forChoice
            checkedProperty=../checkedProperty}}

        {{/with}}
      {{/unless}}
    {{/unless}}
  {{/each}}
</div>
