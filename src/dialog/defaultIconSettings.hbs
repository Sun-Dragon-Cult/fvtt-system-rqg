<form>
  <h2 class="module-header">{{localize "DIALOG.defaultIconSettings.header"}}</h2>

  {{#each this}}
    <div class="form-group">
      <label>
        <span>{{localizeitemtype @key}}</span>
        <img src="{{this}}" />
        {{filePicker target=@key type="image"}}

      </label>
      <div class="form-fields">
        <input type="text" id="{{@key}}" value="{{this}}" name="{{@key}}" />
      </div>
    </div>

  {{/each}}
  <footer class="sheet-footer flexrow">
    <button type="submit" name="submit">
      <i class="fa fa-check"></i>
      OK
    </button>
  </footer>

</form>